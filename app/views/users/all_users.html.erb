 <div class='blocks'>
    <ul>
      <% @users.each do |user| %>
        <li>
          <div class='users_list'>
            <%= user.id %><br>
            <%= user.first_name %><br>
            <%= user.last_name %><br>
            <%= user.email %><br>
            <%= "Created: #{user.created_at}" %><br>
            <%= "Updated: #{user.updated_at}" %>
          </div>
        </li>
    <br>
  <% end %>
    </div>
</ul>
